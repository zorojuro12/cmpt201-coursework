#include <stdio.h>
#include <unistd.h>

int main() {
  int a = getpid();
  printf("Before forking...\n");
  fork();
  if (getpid() == a) {
    printf("This is the parent process with PID : %5d\n", getpid());
  } else {
    printf(
        "THis is the child process with the PID : % 5d, Child PID is : %5d\n",
        getpid(), getppid());
  }

  printf("Done.\n");
}
